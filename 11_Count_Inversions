long long getInversions(long long *arr, int n){
    // Write your code here.

    /*Method 1: Brute Force  T.C=O(N^N) S.C = O(1)
    long long ans = 0;

    for(long long i=0;i<n;i++)
    {
        for(long long j=i+1;j<n;j++)
        {
            if((arr[i] > arr[j]) && (i<j)) ans++;
        }
    }

    return ans;*/
}
