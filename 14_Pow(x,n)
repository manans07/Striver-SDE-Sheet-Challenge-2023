#include <bits/stdc++.h>

int modularExponentiation(int x, int n, int m) {
	// Write your code here.

	
	/*Method 1: Brute Force T.c= O(N) S.C = O(1)
	long long ans = 1;
	for(int i=0;i<n;i++)
	{
		ans *= x;
		ans%=m;
	}

	return (ans%m);*/


	/*Optimal Solution: T.C = O(logN) S.C = O(1) */

	long ans=1;

	long xx=x;

	while(n>0){

    if(n%2!=0)
    {
        ans=((ans)%m*(xx)%m)%m;
    }

    xx=((xx)%m*(xx)%m)%m;
    n=n>>1;

	}

		return (int)(ans%m);

}	



